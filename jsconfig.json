{
  "compilerOptions": {
    "moduleResolution": "node",
    "baseUrl": ".",
    "rootDir": ".",
    "paths": {
      "@root/*": ["./*"],
      "@models/*": ["./app/models/*"],
      "@core/*": ["./core/*"],
      "@middlewares/*": ["./middlewares/*"],
      "@validator": ["./app/validators/index.js"],
      "@services/*": ["./app/services/*"],
      "@lib/*": ["./app/lib/*"],
      "@config": ["./config.js"]
    }
  }
}